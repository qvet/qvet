    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.1",
    "eslint-plugin-prettier": "^5",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.6.0",
