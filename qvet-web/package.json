    "@mui/material": "^5.11.6",
    "@octokit/openapi-types": "^16.0.0",
    "@tanstack/react-query": "^4.23.0",
    "ajv": "^8.12.0",
    "axios": "^1.2.6",
    "dayjs": "1.11.7",
    "fuse.js": "^6.6.2",
    "ky": "^0.33.2",
    "octokit": "^2.0.14",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
